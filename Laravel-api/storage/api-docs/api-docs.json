                    {
                        "bearer": []
                    }
                ]
            }
        },
        "/api/v1/store-albumPet": {
            "post": {
                "tags": [
                    "Albums"
                ],
                "summary": "Create New Album",
                "description": "Create New Product",
                "operationId": "store",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "user_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "category_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "emotion": {
                                        "type": "string",
                                        "example": "wao wao"
                                    },
                                    "image_pet": {
                                        "type": "string",
                                        "example": ""
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Create New Album"
                    },
                    "401": {
                        "description": "UNAUTHORIZED"
                    },
                    "422": {
                        "description": "Resource UNPROCESSABLE"
                    }
                },
                "security": [
                    {
                        "bearer": []
                    }
                ]
            }
        },
